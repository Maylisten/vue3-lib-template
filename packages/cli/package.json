{
  "name": "create-vue3-lib-template",
  "description": "vue3 组件库项目脚手架",
  "keywords": [
    "vue3-lib-template",
    "vue3-lib-template-components",
    "vue",
    "vue3",
    "lib",
    "template",
    "cli"
  ],
  "author": "Mayer",
  "license": "MIT",
  "homepage": "https://maylisten.github.io/vue3-lib-template",
  "files": [
    "lib/*",
    "LICENSE",
    "README.md"
  ],
  "version": "1.1.3",
  "main": "./lib/cli.cjs",
  "module": "./lib/cli.mjs",
  "types": "./lib/cli.d.cts",
  "exports": {
    "require": {
      "types": "./lib/cli.d.cts",
      "default": "./lib/cli.cjs"
    },
    "import": {
      "types": "./lib/cli.d.mts",
      "default": "./lib/cli.mjs"
    }
  },
  "bin": "./lib/cli.js",
  "scripts": {
    "dev": "tsx src/cli",
    "build": "pkgroll --dist lib",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maylisten/vue3-lib-template"
  },
  "bugs": {
    "url": "https://github.com/Maylisten/vue3-lib-template/issues"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "command-line-args": "^6.0.0",
    "command-line-usage": "^7.0.3",
    "download-git-repo": "^3.0.2",
    "fs-extra": "^11.2.0",
    "ora": "5.4.1",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5",
    "typescript": "^5.6.2",
    "validate-npm-package-name": "^6.0.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "^0.8.15",
    "@types/validate-npm-package-name": "^4.0.2",
    "pkgroll": "^2.5.0",
    "tsx": "^4.19.1"
  }
}
